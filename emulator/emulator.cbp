<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="emulator" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/emulator" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add option="-g" />
					<Add option="-D__WXDEBUG__" />
					<Add directory="$(#wx)/lib/gcc_dll/mswd" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="$(#wx)/lib/gcc_dll/mswd" />
				</ResourceCompiler>
				<Linker>
					<Add library="libwxmsw29d_core.a" />
					<Add library="libwxbase29d.a" />
					<Add library="libwxpngd.a" />
					<Add library="libwxzlibd.a" />
					<Add directory="$(#wx)/lib/gcc_dll" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/emulator" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add option="-O2" />
					<Add directory="$(#wx)/lib/gcc_dll/msw" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="$(#wx)/lib/gcc_dll/msw" />
				</ResourceCompiler>
				<Linker>
					<Add option="-s" />
					<Add library="libwxmsw29_core.a" />
					<Add library="libwxbase29.a" />
					<Add library="libwxpng.a" />
					<Add library="libwxzlib.a" />
					<Add directory="$(#wx)/lib/gcc_dll" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-D__GNUWIN32__" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option='[[if (PLATFORM == PLATFORM_MSW &amp;&amp; (GetCompilerFactory().GetCompilerVersionString(_T(&quot;gcc&quot;)) &gt;= _T(&quot;4.0.0&quot;))) print(_T(&quot;-Wno-attributes&quot;));]]' />
