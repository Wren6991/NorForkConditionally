<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="ideFrame">
		<size>800,600</size>
		<fg>wxSYS_COLOUR_BTNFACE</fg>
		<id_arg>0</id_arg>
		<object class="wxBoxSizer" variable="BoxSizer1" member="no">
			<orient>wxVERTICAL</orient>
		</object>
		<object class="wxToolBar" name="ID_TOOLBAR1" variable="ToolBar1" member="yes">
			<style>wxTB_HORIZONTAL|wxTB_NODIVIDER|wxNO_BORDER</style>
			<object class="tool" name="TOOL_NEW" variable="ToolBarItem1" member="yes">
				<label>New</label>
				<bitmap>C:\Users\Owner\Documents\CodeBlocks\norforkconditionally\ide\icons\page_add.png</bitmap>
				<tooltip>New file</tooltip>
			</object>
			<object class="tool" name="TOOL_OPEN" variable="ToolBarItem2" member="yes">
				<label>Open</label>
				<bitmap>C:\Users\Owner\Documents\CodeBlocks\norforkconditionally\ide\icons\folder_page.png</bitmap>
				<tooltip>Open file</tooltip>
			</object>
			<object class="tool" name="TOOL_SAVE" variable="ToolBarItem3" member="yes">
				<label>Save</label>
				<bitmap>C:\Users\Owner\Documents\CodeBlocks\norforkconditionally\ide\icons\disk.png</bitmap>
				<tooltip>Right click for save as</tooltip>
			</object>
			<object class="separator" />
			<object class="tool" name="TOOL_RUN" variable="ToolBarItem4" member="yes">
				<label>Run</label>
				<bitmap>C:\Users\Owner\Documents\CodeBlocks\norforkconditionally\ide\icons\resultset_next.png</bitmap>
				<tooltip>Run in simulator</tooltip>
			</object>
			<object class="tool" name="TOOL_CONNECT" variable="ToolBarItem5" member="yes">
				<label>Connect</label>
				<bitmap>C:\Users\Owner\Documents\CodeBlocks\norforkconditionally\ide\icons\disconnect.png</bitmap>
				<tooltip>Connect to device</tooltip>
			</object>
			<object class="tool" name="TOOL_DOWNLOAD" variable="ToolBarItem6" member="yes">
				<label>Download</label>
				<bitmap>C:\Users\Owner\Documents\CodeBlocks\norforkconditionally\ide\icons\arrow_down.png</bitmap>
				<tooltip>Download to device</tooltip>
			</object>
			<object class="separator" />
			<object class="tool" name="TOOL_ABOUT" variable="ToolBarItem7" member="yes">
				<label>About</label>
				<bitmap>C:\Users\Owner\Documents\CodeBlocks\norforkconditionally\ide\icons\help.png</bitmap>
				<tooltip>About this software</tooltip>
				<handler function="OnAbout" entry="EVT_TOOL" />
