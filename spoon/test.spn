const pointer debugout = 0xc000;
const pointer debugin = 0xc001;
const pointer true = 0x7d00;        // constant ff
const pointer false = 0x7d01;       // constant 0

macro output(src, dest)
{
    var int x;
    nfc(x, val(0xff));
    nfc(x, src);
    nfc(dest, x);
}

macro logical_not(src, dest)
{
    nfc(dest, val(0xff));
    nfc(dest, src);
}


function main()
{
    var int x;
    var pointer p;
    x = 5;
    p = 0x1234;
    while (true)
    {
        output(debugin, debugout);
    }
}
